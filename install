<?php

use PhpMx\TerminalInstall;

return new class extends TerminalInstall {

    function __invoke()
    {
        $this->promote('system/view/front/layout/default.html');
        $this->promote('system/view/front/base.html');
        $this->promote('system/view/front/error.html');
        $this->promote('system/view/front/script.js');
        $this->promote('system/view/front/style.css');
    }
};
