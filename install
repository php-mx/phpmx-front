<?php

use PhpMx\TerminalInstall;

return new class extends TerminalInstall {

    function __invoke()
    {
        $this->promote('system/view/front/_content.html');
        $this->promote('system/view/front/_script.js');
        $this->promote('system/view/front/_style.css');
        $this->promote('system/view/front/layout/default/_content.html');
        $this->promote('system/view/page/error/_content.html');
    }
};
