<?php

use PhpMx\TerminalInstall;

return new class extends TerminalInstall {

    function __invoke()
    {
        $this->promote('system/view/front/base/base.css');
        $this->promote('system/view/front/base/base.html');
        $this->promote('system/view/front/base/base.js');

        $this->createDir('library/icons');
    }
};
